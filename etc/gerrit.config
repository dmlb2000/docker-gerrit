[gerrit]
	basePath = git
	canonicalWebUrl = http://@@GERRIT_ADDR@@:8080/
[database]
	type = mysql
	hostname = @@MYSQL_ADDR@@
	database = gerrit
	username = gerrit
[index]
	type = LUCENE
[auth]
	type = @@AUTH_TYPE@@
[sendemail]
	smtpServer = @@MAIL_ADDR@@
	smtpUser = @@MAIL_USER@@
	smtpPass = @@MAIL_PASS@@
[container]
	user = root
	javaHome = /usr/lib/jvm/java-8-openjdk-amd64/jre
[sshd]
	listenAddress = *:29418
[httpd]
	listenUrl = http://*:8080/
[cache]
	directory = cache
